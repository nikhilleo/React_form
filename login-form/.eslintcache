lue\n    }))\n    console.log(Form);\n  }\n\n\n  return (\n    <div className=\"App\">\n      <div className=\"display__form\" >\n        <form className=\"register\" onSubmit={handleSubmit}>\n            <label htmlFor=\"name\">Full Name</label>\n            <input type=\"text\" name=\"fname\" id=\"name\" placeholder=\"Enter Name\" onChange={handleChange} />\n            <label htmlFor=\"email\">Email</label>\n            <input type=\"text\" name=\"email\" id=\"email\" placeholder=\"example@gmail.com\" onChange={handleChange} />\n            <label htmlFor=\"password\">Enter Password</label>\n            <input type=\"password\" name=\"password\" id=\"password\" placeholder=\"Atleast 7 Characters\" onChange={handleChange} />\n            <label htmlFor=\"mobile\">Mobile Number</label>\n            <input type=\"text\" maxLength=\"10\" name=\"mobile\" id=\"mobile\" placeholder=\"Enter Mobile Number\" onChange={handleChange} />\n            <button className=\"sub__button\" type=\"submit\">Submit</button>\n        </form>\n      </div>\n    </div>\n  );\n}\n\nexport default Register;\n","/home/nikhil/Desktop/SOAL/Minims/Milestone7/nikhilleo_tep-challenge-80008-react-forms-events/login-form/src/Profile.js",["34"],"/home/nikhil/Desktop/SOAL/Minims/Milestone7/nikhilleo_tep-challenge-80008-react-forms-events/login-form/src/axios.js",[],{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","severity":1,"message":"40","line":9,"column":3,"nodeType":"41","messageId":"42","endLine":9,"endColumn":11},{"ruleId":"39","severity":1,"message":"43","line":4,"column":10,"nodeType":"41","messageId":"42","endLine":4,"endColumn":14},{"ruleId":"44","severity":1,"message":"45","line":21,"column":18,"nodeType":"46","messageId":"47","endLine":21,"endColumn":20},{"ruleId":"44","severity":1,"message":"45","line":33,"column":22,"nodeType":"46","messageId":"47","endLine":33,"endColumn":24},"no-native-reassign",["48"],"no-negated-in-lhs",["49"],"no-unused-vars","'R